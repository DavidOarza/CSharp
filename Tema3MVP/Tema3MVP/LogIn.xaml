<Window x:Class="Tema3MVP.LogIn"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tema3MVP"
        mc:Ignorable="d"
        Title="LogIn" Height="450" Width="800"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Label Content="Email:" HorizontalAlignment="Left" Margin="48,121,0,0" VerticalAlignment="Top" Width="184" Height="41"/>
        <Label Content="Parola:" HorizontalAlignment="Left" Margin="48,167,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.44,0.621" Width="184" Height="41"/>
        <TextBox x:Name="txtEmail" HorizontalAlignment="Left" Height="41" Margin="237,121,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="260"/>
        <TextBox x:Name="txtParola" HorizontalAlignment="Left" Height="41" Margin="237,167,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="260"/>
        <Button x:Name="btnInapoi" Content="Inapoi" HorizontalAlignment="Left" Height="44" Margin="600,366,0,0" VerticalAlignment="Top" Width="184" Click="btnInapoi_Click"/>
        <Button x:Name="btnConectare" Content="LogIn" HorizontalAlignment="Left" Height="44" Margin="48,366,0,0" VerticalAlignment="Top" Width="184"/>

    </Grid>
</Window>
